programa ejemplo
areas
  ciudad: AreaC (1,1,100,100)
robots
  robot robot1
  variables
    papeles:numero
  comenzar
    Pos(1,2)
    derecha
    papeles:=0
    mientras (papeles <10) & (PosAv < 100)
      mientras (HayPapelEnLaEsquina) & (papeles <10)
        tomarPapel
        papeles:=papeles+1
      mover
  fin
variables
  R_info: robot1
comenzar
  AsignarArea(R_info, ciudad)
  Iniciar(R_info, 1,1)
fin