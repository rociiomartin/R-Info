programa ej3
areas
  ciudad: AreaC (1,1,100,100)
robots
  robot robot1
  comenzar
    Pos(1,45)
    derecha
    repetir 49
      depositarPapel
      mover
      depositarFlor
      mover
    depositarPapel
    mover
    depositarFlor
  fin
variables
  R_info: robot1
comenzar
  AsignarArea(R_info, ciudad)
  Iniciar(R_info, 1,1)
fin
