programa ej1
procesos
  proceso cuadrado (E lado : numero)
  comenzar
    repetir 4
      repetir lado
        mover
      derecha
  fin
areas
  ciudad: AreaC (1,1,100,100)
robots
  robot robot1
  variables
    lado:numero
  comenzar
    Pos(2,1)
    lado:=1
    repetir 5
      cuadrado(lado)
      Pos(PosAv+lado,PosCa)
      lado:=lado+1     
  fin
variables
  R_info: robot1
comenzar
  AsignarArea(R_info, ciudad)
  Iniciar(R_info, 1,1)
fin