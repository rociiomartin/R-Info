programa ej1
procesos
  proceso rectangulo (E ancho : numero; E alto : numero)
  comenzar
    repetir 2
      repetir alto
        mover
      derecha
      repetir ancho
        mover
      derecha
  fin
areas
  ciudad: AreaC (1,1,100,100)
robots
  robot robot1
  variables
    ancho,alto:numero
  comenzar
    ancho:=5
    alto:=1
    repetir 3
      rectangulo (ancho,alto)
      ancho:=ancho-2
      Pos(PosAv+1,PosCa+2)
  fin
variables
  R_info: robot1
comenzar
  AsignarArea(R_info, ciudad)
  Iniciar(R_info, 1,1)
fin